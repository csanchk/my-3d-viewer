{
  "name": "@matterport/sdk",
  "version": "1.4.24",
  "description": "Matterport SDK and SDK utilities.",
  "main": "dist/mp-sdk.js",
  "types": "dist/types/mp-sdk.d.ts",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "webpack --config ./webpack.config.js --no-devtool --env production && yarn build-package-types",
    "build-package-types": "dts-bundle-generator --config dts.embed.package.js",
    "clean": "rm -rf ./dist",
    "dev": "webpack serve --config ./webpack.config.js --env development",
    "prepare-public": "yarn replace-mp",
    "replace-mp": "replace-in-file /@matterport/g @matterport *.*,**/*.* --isRegex"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@matterport/webpack": "^1.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "dts-bundle-generator": "^7.2.0",
    "replace-in-file": "^6.3.5",
    "three": "^0.151.3",
    "typescript": "^4.6.3",
    "webpack": "^5.75.0"
  }
}
